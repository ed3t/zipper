{
    "name": "Usefulsomebody/PhpZipper",
    "description": "Wannabe successor of Chumper/Zipper package for Laravel",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Abednego Edet",
            "email": "asapabedi@gmail.com",
            "homepage": "https://edet.dev",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.2.0",
        "illuminate/support": "^6.18|^7.0|^8.0",
        "illuminate/filesystem": "^6.18|^7.0|^8.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.3",
        "orchestra/testbench": "^5.1",
        "phpunit/phpunit": "^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "Usefulsomebody\\PhpZipper\\": "src/Usefulsomebody/Php-Zipper"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Usefulsomebody\\PhpZipper\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit --colors=always",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Usefulsomebody\\PhpZipper\\PhpZipperServiceProvider"
            ],
            "aliases": {
                "Zipper": "Usefulsomebody\\PhpZipper\\PhpZipper"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
